<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.tlilektik.oselottest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/jaguar"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/jaguar"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity
                android:name=".iu.LauncherActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".iu.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.Main">
        </activity>
        <activity
                android:name=".iu.DetailActivity"
                android:theme="@style/AppTheme2.Detail">
        </activity>
        <activity
                android:name=".iu.LoginActivity"
                android:label="@string/app_name">
        </activity>

    </application>

</manifest>